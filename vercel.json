{
    "headers": [
      {
        "source": "/(.*)",
        "headers" : [
          {
            "key" : "X-Content-Type-Options",
            "value" : "nosniff"
          },
          {
            "key" : "X-Frame-Options",
            "value" : "DENY"
          },
          {
            "key" : "X-XSS-Protection",
            "value" : "1; mode=block"
          },
          {
            "key" : "Referrer-Policy",
            "value" : "strict-origin"
          },
          {
            "key" : "Permissions-Policy",
            "value" : "geolocation=(self), microphone=()"
          },
          {
            "key" : "Content-Security-Policy-Report-Only",
            "value" : "default-src 'self'; font-src 'self'; img-src 'self'; script-src 'self'; style-src 'self'; frame-ancestors 'self'; report-uri https://umaproject.uriports.com/reports/report; report-to default"
          }
        ]
      }
    ]
  }
